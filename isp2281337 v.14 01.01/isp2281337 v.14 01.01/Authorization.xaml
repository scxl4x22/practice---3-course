<Window x:Class="isp2281337_v._14_01._01.Authorization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:isp2281337_v._14_01._01"
        mc:Ignorable="d"
        Title="Authorization" Height="155" Width="130" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <PasswordBox x:Name="inputPassword" HorizontalAlignment="Left" Margin="15,33,0,0" VerticalAlignment="Top" Width="85" Height="20" PreviewKeyDown="input_PreviewKeyDown" KeyDown="input_KeyDown"/>
        <Button IsDefault="True" x:Name="enter" Content="Войти" HorizontalAlignment="Left" Margin="15,64,0,0" VerticalAlignment="Top" Width="85" Click="enter_Click"/>
        <Button IsCancel="True" x:Name="cancel" Content="Отмена" HorizontalAlignment="Left" Margin="15,93,0,0" VerticalAlignment="Top" Width="85" Click="cancel_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="35,3,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="50"><Run Language="ru-ru" Text="Пароль:"/></TextBlock>

    </Grid>
</Window>
